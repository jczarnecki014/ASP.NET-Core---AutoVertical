                <h5 class="mb-3">Cargo</h5>
                <div class="row">
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">Allowe package</label>
                        <input asp-for="truck.AllowPackage" type="text" class="form-control p-3" id="" placeholder="kg">
                        <span asp-validation-for="truck.AllowPackage" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">permissible gross weight</label>
                        <input asp-for="truck.PermissibleGrossWeight" type="text" class="form-control p-3" id="" placeholder="kg">
                        <span asp-validation-for="truck.PermissibleGrossWeight" class="text-danger"></span>
                    </div> 
                </div>
            </div>

                @*Photo*@

            <div class="col-12 mt-5">
                <h5 class="mb-3">Photos</h5>
                <div class="row">
                    <div class="col-lg-12 p-3 form-group">
                         <label class="form-label">Multiple files input example</label>
                         <input id="filesInput" class="form-control bg-warning" type="file" name="files" multiple>    
                    </div>
                </div>

                <div id="output" class="row">
                </div>

            </div>

                @*Vehicle description*@

            <div class="col-12 mt-5">
                <h5 class="mb-3">Vehicle description</h5>
                <div class="row">
                    <div class="col-lg-12 p-3 form-group">
                        <label for="">Announcement Title</label>
                        <input asp-for="Title" type="text" class="miniPreviewInfo form-control p-3" id="Title" placeholder="First owner,perfect condition">
                        <span asp-validation-for="Title" class="text-danger"></span>
                    </div>
                    <div class="col-lg-12 p-3 form-group">
                        <label for="">Description</label>
                        <textarea asp-for="Description" class="miniPreviewInfo form-control" id="Description" rows="10"></textarea>
                        <p class="text-end"><smal><span id="textLength">0</span>/4000</smal></p>
                        <span asp-validation-for="Description" class="text-danger"></span>
                    </div>
                </div>
            </div>

