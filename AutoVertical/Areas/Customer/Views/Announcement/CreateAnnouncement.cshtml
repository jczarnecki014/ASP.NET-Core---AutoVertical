@using AutoVertical_Model.Models;
@model Vehicle
<div class="container">
    <div class="row">
        <section name="form" class="col-lg-7">
            <form method="post" enctype="multipart/form-data">
            <h1>Create announcement</h1>
            <div class="col-12 pt-4">
                    <select asp-for="VehicleType" id="VehicleSelector" class="miniPreviewInfo form-control">
                        <option value="car">Cars</option>
                        <option value="motorcycle">Motorcycles</option>
                        <option value="truck">Trucks</option>
                    </select>
            </div>

            @*Main quality*@
            <div class="container">
                <div class="col-12 mt-5">
                <h3>Vehicle details</h3>
                <h5 class="mb-3">Main quality</h5>
                <div class="row">
                    <div class="col-lg-6 form-check border border-1 p-3">
                        <input asp-for="Damaged" class="form-check-input" type="checkbox" value="true" id="damaged">
                        <label class="form-check-labe" for="damaged">
                            Damaged
                        </label>
                    </div>
                    <div class="col-lg-6 btn-group" role="group">
                        <input asp-for="Imported" type="radio" class="btn-check" value="Imported" id="btnradio1">
                        <label class="btn btn-outline-primary" for="btnradio1">Imported</label>
                        <input asp-for="Imported" type="radio" class="btn-check" value="Domestic" id="btnradio2">
                        <label class="btn btn-outline-primary" for="btnradio2">Domestic</label>
                        <span asp-validation-for="Imported" class="text-danger"></span>
                    </div>

                    <div class="car col-lg-6 form-check border border-1 p-3 mt-2">
                        <input asp-for="car.RightHandDrive" class="form-check-input" type="checkbox" value="true" id="Righthanddrive">
                        <label class="form-check-labe" for="Righthanddrive">
                            Right hand drive ( UK )
                        </label>
                    </div>
                    <div class="truck col-lg-6 form-check border border-1 p-3 mt-2">
                        <input asp-for="truck.NonStandarVehicle" class="form-check-input" type="checkbox" value="Yes" id="Nonstandardvehicle">
                        <label class="form-check-labe" for="Nonstandardvehicle">
                            Non standard vehicle
                        </label>
                    </div>
                    <div class="truck col-lg-6 form-check border border-1 p-3 mt-2">
                        <input asp-for=truck.DoubleRearWheels class="form-check-input" type="checkbox" value="Yes" id="Doublerearwheels">
                        <label class="form-check-labe" for="Doublerearwheels">
                           Double rear wheels
                        </label>
                    </div>
                </div>
            </div>
            </div>

            @*Basics information*@

            <div class="col-12 mt-5">
                <h5 class="mb-3">Basic Information - first registration</h5>
                <div class="row">
                    <div class="col-lg-6 p-3 form-group">
                        <label for="Vin">Vin number</label>
                        <input asp-for=VinNumber type="text" class="miniPreviewInfo form-control p-3" id="Vin" placeholder="WAUZZZ5432FDS45234DX">
                        <span asp-validation-for="VinNumber" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="milage">Milage</label>
                        <input asp-for="Milage" class="miniPreviewInfo form-control p-3" id="Milage" placeholder="100 000 km">
                        <span asp-validation-for="Milage" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="registration">vehicle registration number</label>
                        <input asp-for="VehicleRegistrationNumber" class="miniPreviewInfo form-control p-3" id="RegistrationNumber" placeholder="DJE 4132">
                        <span asp-validation-for="VehicleRegistrationNumber" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <div class="row">
                            <div class="col-lg-4 p-3 form-group">
                                <label for="regDay">Day</label>
                                <input asp-for="RegDay" class="miniPreviewInfo form-control" id="RegDay" placeholder="DD">
                                <span asp-validation-for="RegDay" class="text-danger"></span>
                            </div>
                            <div class="col-lg-4 p-3 form-group">
                                <label for="regMon">Month</label>
                                <input asp-for="RegMonth" class="miniPreviewInfo form-control" id="RegMon" placeholder="MM">
                                <span asp-validation-for="RegMonth" class="text-danger"></span>
                            </div>
                            <div class="col-lg-4 p-3 form-group">
                                <label for="regYear">Year</label>
                                <input asp-for="RegYear" class="miniPreviewInfo form-control" id="RegYear" placeholder="YYYY">
                                <span asp-validation-for="RegYear" class="text-danger"></span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

                @*Technical Data*@

             <div class="col-12 mt-5">
                <h5 class="mb-3">Technical data</h5>
                <p class="text-secondary"><smal>Warning ! Please to well cheeck vehicle data before adding announcement !! </small></p>
                <div class="row">
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">Production year</label>
                        <select asp-for="ProductionYear" class="miniPreviewInfo form-control p-3" id="ProductionYear">
                            <option selected>--Select--</option>
                        </select>
                        <span asp-validation-for="ProductionYear" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">Brand</label>
                        <select asp-for="Brand" class="miniPreviewInfo specific form-control p-3" id="Brand">
                            <option selected>BMW</option>
                        </select>
                        <span asp-validation-for="Brand" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">Model</label>
                        <input asp-for="Model" type="text" class="miniPreviewInfo specific form-control p-3" id="Model" placeholder="Model">
                        <span asp-validation-for="Model" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">Fuel</label>
                        <select asp-for="Fuel" class="miniPreviewInfo specific form-control p-3" id="Fuel">
                            <option selected>Petrol</option>
                        </select>
                        <span asp-validation-for="Fuel" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">Power</label>
                        <input asp-for="Power" type="number" class="miniPreviewInfo form-control p-3" id="Power" placeholder="150 km">
                        <span asp-validation-for="Power" class="text-danger"></span>
                    </div>
                     <div class="col-lg-6 p-3 form-group">
                        <label for="">Cubic capacity</label>
                        <input asp-for="CubicCapacity" type="text" class="miniPreviewInfo form-control p-3" id="CubbicCapacity" placeholder="1 800 cm">
                        <span asp-validation-for="CubicCapacity" class="text-danger"></span>
                    </div>
                    <div class="car col-lg-6 p-3 form-group">
                        <label for="">Number of door</label>
                        <select asp-for="car.NumberOfDoor" class="specific form-control p-3">
                            <option selected>5 - doors</option>
                        </select>
                        <span asp-validation-for="car.NumberOfDoor" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">Gear box</label>
                        <select asp-for="GearBox" class="miniPreviewInfo specific form-control p-3" id="GearBox">
                            <option selected>Manual</option>
                        </select>
                        <span asp-validation-for="GearBox" class="text-danger"></span>
                    </div>
                    <div class="car motorcycle truck col-lg-6 p-3 form-group">
                        <label for="">Drive</label>
                        <select asp-for="Drive" class="miniPreviewInfo specific form-control p-3" id="Drive">
                            <option selected>Front-wheel drive (FWD)</option>
                        </select>
                        <span asp-validation-for="Drive" class="text-danger"></span>
                    </div>
                     <div class="col-lg-6 p-3 form-group">
                        <label for="">Co2 Emision</label>
                        <input type="text" class="form-control p-3" id="" placeholder="g/km">
                    </div>
                </div>
            </div>


                @*Body*@

             <div class="col-12 mt-5">
                <h5 class="mb-3">Body</h5>
                <div class="row">
                    <div class="col-lg-12 p-3 form-group">
                        <label for="">Body type</label>
                        <select asp-for="BodyType" class="miniPreviewInfo specific form-control p-3" id="BodyType">
                            <option selected>Sedan</option>
                        </select>
                        <span asp-validation-for="BodyType" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">Color</label>
                        <input asp-for="Color" type="text" class="miniPreviewInfo form-control p-3" id="Color" placeholder="Silver">
                        <span asp-validation-for="Color" class="text-danger"></span>
                    </div>
                    <div class="col-lg-6 p-3 form-group">
                        <label for="">Color type</label>
                        <select asp-for="ColorType" class="form-control p-3">
                            <option selected>Metalic</option>
                        </select>
                        <span asp-validation-for="ColorType" class="text-danger"></span>
                    </div>
                    <div class="truck col-lg-6 p-3 form-group">
                        <label for="">Number of axles</label>
                        <input asp-for="truck.NumberOfAxles" type="text" class="form-control p-3" id="" placeholder="3">
                        <span asp-validation-for="truck.NumberOfAxles" class="text-danger"></span>
                    </div>
                    <div class="car col-lg-6 p-3 form-group">
                        <label for="">Number of seats</label>
                        <select asp-for="car.NumberOfSeats" class="specific form-control p-3">
                            <option selected>5</option>
                        </select>
                        <span asp-validation-for="car.NumberOfSeats" class="text-danger"></span>
                    </div>
                </div>
            </div>

                @*Cargo - TRUCK*@

